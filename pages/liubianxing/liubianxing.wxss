.boxF,
.boxS,
.boxT {
    width: 80px;
    height: 100px;
    overflow: hidden;
}

.boxF,
.boxS {
    visibility: hidden;
}

.boxF {
    transform: rotate(120deg);
    float: left;
}

.boxS {
    transform: rotate(-60deg);
}

.boxT {
    transform: rotate(-60deg);
    /* background: no-repeat 50% center; */
    /* background-size: 125% auto; */
    visibility: visible;
    background-repeat: no-repeat;
    /* background-image: url('img/strawberry.jpg'); */
    background-color: #b2f3fb;
}

.pic-item {
    margin-left: 4px;
    display: inline-block;
}

.pic-item:after {
    content: '';
    height: 0;
    line-height: 0;
    display: block;
    visibility: hidden;
    clear: both;
}

