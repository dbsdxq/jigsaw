.line1 {
    position: relative;
}

.line2 {
    position: relative;
    top: -31px;
    left: 42px;
}

.line3 {
    position: relative;
    top: -62px;
    left: 0;
}

.line4 {
    position: relative;
    top: -93px;
    left: 42px;
